  "version": "0.2.0",
  "configurations": [
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "Run editor",
      "cwd": "${workspaceFolder}/",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "command": "neu run",
      "preLaunchTask": "BuildEditor"
    },
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "Debug editor",
      "cwd": "${workspaceFolder}/",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "command": "neu run -- --window-enable-inspector=true",
      "preLaunchTask": "BuildEditor"
    },
  ]
}