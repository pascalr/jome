class TestEq {
  constructor: |actual, expected, message| => (
    @pass = actual == expected
    if (@pass) (
      console.log("\x1b[32mRÉUSSI\x1b[0m", message || expected)
    ) else (
      console.log("\x1b[31mÉCHEC:\x1b[0m", message, "\nExpected:", expected, "\nActually:", actual);
    )
  )
}

