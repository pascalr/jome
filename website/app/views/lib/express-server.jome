import {AppPage} from '../../lib/app.jome'

main {
  AppPage title: 'Jome express server', content: <md>

  # Express Server

  ExpressServer is a wrapper for express in Jome.

  ## Usage

  ```jome
  {
    ExpressServer port: 3000
      useStatic '/jome', 'docs'
      get '/', |req, res| => (
        res.redirect('/jome')
      )
  }
  run
  ```

</md>
