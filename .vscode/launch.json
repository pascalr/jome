  "version": "0.2.0",
  "configurations": [

    {
      "type": "node",
      "request": "launch",
      "name": "Run server",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": ["-r"],
      "program": "${workspaceFolder}/jome.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run tests",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": ["test.jome"],
      "program": "${workspaceFolder}/jome.js"
    },
  ]
}